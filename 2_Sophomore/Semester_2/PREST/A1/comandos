exercise1<- read.table("msoftw_data.txt",header=TRUE,sep=",", dec="," , strip.white=TRUE)
length(exercise1$virus)
library(summarytools)
freq(exercise1$virus,cumul=FALSE)
barplot(table(exercise1),main="MSOFTW",xlab="Virus",ylab="absolut_frecuency")
exercise2<- read.table("transistor_data.txt",header=TRUE,sep=",", dec=".", strip.white=TRUE)
library("car")
scatterplot(x~y , regLine=FALSE, smooth=FALSE, boxplots=FALSE, data=exercise2, xlab="Voltage (V)", ylab="Current (mA)", pch=16)
cor(exercise2$x,exercise2$y)
exercise2Model<-lm(x~y,data=exercise2)
summary(exercise2Model)
scatterplot(x~y , regLine=TRUE, smooth=FALSE, boxplots=FALSE, data=exercise2, xlab="Voltage (V)", ylab="Current (mA)", pch=16)
cor(exercise2$x,exercise2$y)*cor(exercise2$x,exercise2$y)
